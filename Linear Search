/*package whatever //do not write package name here */

import java.io.*;

class GFG {
    public int search(int a[],int x){
        int n=a.length;
        for(int i=0;i<n;i++){
            if(a[i]==x)
            return i;
        }
        return -1;
    }
	public static void main (String[] args) {
int a[]={1,4,5,6};
int x=6;
GFG k=new GFG();
int res=k.search(a,x);
if(res==-1)
System.out.println("not found ");
else
System.out.println(res);
}
}
